alias/crea pbck sqrt(pxbck**2+pybck**2+pzbck**2)
alias/crea thetabck acos(pzbck/sqrt(pxbck**2+pybck**2+pzbck**2))
alias/create rbck sqrt(xbck**2+ybck**2)
alias/crea p1 sqrt(pxqua1**2+pyqua1**2+pzqua1**2)
alias/crea theta1 acos(pzqua1/sqrt(pxqua1**2+pyqua1**2+pzqua1**2))
alias/create r1 sqrt(xqua1**2+yqua1**2)
alias/crea p2 sqrt(pxqua2**2+pyqua2**2+pzqua2**2)
alias/crea theta2 acos(pzqua2/sqrt(pxqua2**2+pyqua2**2+pzqua2**2))
alias/create r2 sqrt(xqua2**2+yqua2**2)
